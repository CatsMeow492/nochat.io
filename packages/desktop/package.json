{
  "name": "nochat-desktop",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:frontend": "npm run --prefix ../web build:desktop && shx mkdir -p dist && shx cp -r ../web/out/* dist/",
    "tauri": "tauri",
    "dev": "npm run build:frontend && tauri dev",
    "build": "npm run build:frontend && tauri build",
    "build:debug": "npm run build:frontend && tauri build --debug"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "shx": "^0.4.0"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-deep-link": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "@tauri-apps/plugin-notification": "^2.0.0",
    "@tauri-apps/plugin-opener": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0"
  }
}
